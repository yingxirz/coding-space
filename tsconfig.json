{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@/*": ["src/*"],
      "@coding-space": ["src"],
      "@coding-space/*": ["src/*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*"]
}
